{
  "compilerOptions": {
    "module": "ES2022",
    "target": "ES2022",
    "lib": [
      "ES2022",
      "dom"
    ],
    "jsx": "react",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "i18next-http-backend": [
        "./index.d.mts"
      ]
    },
    "types": [],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowImportingTsExtensions": true
  },
  "include": [
    "./index.d.mts",
    "./test/**/*",
    "./verify-integration.ts"
  ],
  "exclude": []
}